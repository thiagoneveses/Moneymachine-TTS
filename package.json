{
 "name": "TTS-Translator-Moneymachine",
 "version": "1.0.0",
 "description": "Text to Speech Translator Desktop App",
 "author": "Thiago Neves",
 "main": "main.js",
 "scripts": {
   "start": "electron .",
   "build:win": "electron-builder --win",
   "build:mac": "electron-builder --mac",
   "publish": "electron-builder --win --publish always"
 },
 "dependencies": {
   "@google/generative-ai": "^0.21.0",
   "electron-updater": "^6.1.7",
   "electron-log": "^5.0.1"
 },
 "devDependencies": {
   "electron": "28.0.0",
   "electron-builder": "24.9.1"
 },
 "repository": {
   "type": "git",
   "url": "https://github.com/thiagoneveses/Moneymachine-TTS.git"
 },
 "build": {
   "publish": [{
     "provider": "github",
     "owner": "thiagoneveses",
     "repo": "Moneymachine-TTS"
   }],
   "appId": "com.tts.moneymachine",
   "productName": "TTS Moneymachine",
   "directories": {
     "output": "release"
   },
   "win": {
     "target": "nsis",
     "icon": "assets/icon.ico"
   },
   "mac": {
     "target": "dmg",
     "icon": "assets/icon.icns"
   }
 }
}